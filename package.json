{
  "name": "@pfadimh/hitobito-api",
  "version": "0.1.0",
  "description": "Type-safe TypeScript wrapper for the Hitobito REST API",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "keywords": ["hitobito", "api", "pfadi", "scout", "typescript"],
  "author": "PfadiMH",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/PfadiMH/hitobito-api.git"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.7.0",
    "vitest": "^2.0.0"
  },
  "dependencies": {
    "zod": "^3.24.0"
  }
}
